{
    "manifest_version": 3,
    "name": "UoN Moodle Dark Mode",
    "version": "0.1.0",
    "description": "Overrides UoN Moodle's theme with a dark theme",

    "content_scripts": [{
        "matches": ["https://cms.bits-hyderabad.ac.in/*"],
        "js": ["dark.js"]
        }],

    "web_accessible_resources": [{
            "resources": ["dark.css"],
            "matches": ["https://cms.bits-hyderabad.ac.in/*"]
        }]
}
